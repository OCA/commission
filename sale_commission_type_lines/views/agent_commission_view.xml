<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="agent_commission_form_view" model="ir.ui.view">
        <field name="name">agent.commission.form</field>
        <field name="model">agent.commission</field>
        <field name="arch" type="xml">
            <form string="Agent Commission">
                <sheet>
                    <group>
                        <group>
                            <field name="sequence" />
                            <field name="agent_id" />
                            <field name="commission_id" />
                            <field name="name" invisible="1" />
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="agent_commission_tree_view" model="ir.ui.view">
        <field name="name">agent.commission.tree</field>
        <field name="model">agent.commission</field>
        <field name="arch" type="xml">
            <tree string="Commissions" editable="bottom">
                <field name="sequence" widget="handle" />
                <field name="agent_id" options="{'no_create_edit': True}" />
                <field name="commission_id" options="{'no_create_edit': True}" />
                <field name="name" invisible="1" />
            </tree>
        </field>
    </record>

    <record id="agent_commission_item_action" model="ir.actions.act_window">
        <field name="name">Agent Commissions</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">agent.commission</field>
        <field name="view_mode">tree,form</field>
    </record>

